extends layout

block content
  .row
    .col-xs-4
      a(href='http://tienganhthaytien.com/')
        img(alt='Brand', src='/brand-transparent.png', style='width:55px;height:40px;')
    .col-xs-8
      ul.list-inline.pull-right
        li
          a(href='http://tienganhthaytien.com/') Trang chủ
        li
          a(href='http://tienganhthaytien.com/dang-ky-hoc/') Đăng ký học
        li
          a(href='http://tienganhthaytien.com/cach-hoc-link-tai-phim/') Tự học
        li
          a(href='http://tienganhthaytien.com/lien-he/') Liên hệ
    .col-xs-12
      .jumbotron.text-center
        h2 Type word & press Enter
        .col-xs-8.col-xs-offset-2.well
          span.input-group.col-xs-12
            input.form-control#s(type='text', placeholder="Search for...", autofocus)
  .row
    .col-xs-12
      #resultsDiv
        #oald.col-xs-6
        #spinning.col-xs-3
        #vndic.col-xs-3
